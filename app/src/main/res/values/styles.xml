<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="backtoast">
        <item name="stTextBold">true</item>
        <item name="stTextColor">#fff</item>
        <item name="stFont">@font/nanumsquareroundr</item>
        <item name="stTextSize">14sp</item>
        <item name="stColorBackground">#FF636363</item>
        <item name="stSolidBackground">true</item>
        <item name="stIconStart">@drawable/login_id</item>
        <item name="stRadius">5dp</item>
    </style>

    <style name="sign">
        <item name="stTextBold">true</item>
        <item name="stTextColor">#fff</item>
        <item name="stFont">@font/nanumsquareroundr</item>
        <item name="stTextSize">14sp</item>
        <item name="stColorBackground">#FF636363</item>
        <item name="stSolidBackground">true</item>
        <item name="stIconStart">@drawable/login_id</item>
        <item name="stRadius">5dp</item>
    </style>


</resources>
